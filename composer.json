{
    "name": "flaphl/reissue",
    "description": "handles serializing and deserializing data structures, including, into array structures or other formats like XML and JSON.",
    "type": "library",
    "license": "MIT",
    "keywords": ["reissue", "serialize", "flaphl", "php"],
    "authors": [
        {
            "name": "Jade Phyressi",
            "email": "jade@flaphl.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "flaphl/deprecation-contracts": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0",
        "phpstan/phpdoc-parser": "^1.2",
        "flaphl/cache": "^1.0.1",
        "flaphl/support": "^1.0.0",
        "flaphl/error-handler": "^1.0.1",
        "flaphl/filesystem": "^1.0.0",
        "flaphl/injection": "^1.0.0"
    },
    "autoload": {
        "psr-4": { "Flaphl\\Element\\Reissue\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}